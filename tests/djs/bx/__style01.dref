weak (~sty: Empty > lStyPro)

val theStyPro :: {

  getFontSize : (this:{Ref(~sty)|(canStyle v)}) -> Str

, setFontSize : (this:{Ref(~sty)|(canStyle v)}, Str) -> {Ref(~sty)|(canStyle v)}

, _ : Bot

}

heap (
  lStyPro : theStyPro > lROOT
)
